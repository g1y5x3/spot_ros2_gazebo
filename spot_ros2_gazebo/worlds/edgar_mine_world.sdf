            <mesh>
              <uri>meshes/Edgar_Section_04.DAE</uri>
            </mesh>
          </geometry>
        </collision>
        <visual name="section_04_visual">
          <geometry>
            <mesh>
              <uri>meshes/Edgar_Section_04.DAE</uri>
            </mesh>
          </geometry>
        </visual>

        <collision name="section_05_collision">
          <geometry>
            <mesh>
              <uri>meshes/Edgar_Section_05.DAE</uri>
