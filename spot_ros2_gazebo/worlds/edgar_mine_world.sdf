        <collision name="section_06_collision">
          <geometry>
            <mesh>
              <uri>meshes/Edgar_Section_06.DAE</uri>
            </mesh>
          </geometry>
        </collision>
        <visual name="section_06_visual">
          <geometry>
            <mesh>
